{
  "name": "go-infrastructure",
  "version": "0.1.0",
  "description": "Infrastructure for the Go Mobile App",
  "main": "src/main.ts",
  "repository": "https://github.com/asattelmaier/go-session-server/infrastructure",
  "author": "Alexander Sattelmaier",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "tsc && cdktf synth",
