  },
  "engines": {
    "node": ">=18.0"
  },
  "dependencies": {
    "@cdktf/provider-google": "12.0.0",
    "cdktf": "^0.19.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "cdktf-cli": "^0.19.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
